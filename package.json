{
  "name": "node-log-info",
  "version": "1.1.1",
  "description": "node日志服务",
  "main": "index.js",
  "keywords": [
    "node",
    "logger",
    "logs"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/fanguyun214/node-logger"
  },
  "scripts": {
    "dev": "nodemon server.js",
    "build": "babel --extensions '.ts' src --out-dir lib",
    "lint": "tslint -p tsconfig.json",
    "log": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "test": "jest --passWithNoTests"
  },
  "author": "JasonFan <2896962690@qq.com>",
  "license": "MIT",
  "dependencies": {
    "os": "^0.1.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@types/koa": "^2.11.3",
    "@types/node": "^14.0.27",
    "@types/winston": "^2.4.4",
    "koa": "^2.13.0",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.3",
    "typescript": "^3.9.7",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "commitizen": "4.0.4",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "4.2.5",
    "lint-staged": "^10.2.11"
  },
  "optionalDependencies": {},
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{,tsx,ts,js,json,css,md}": [
      "prettier --write",
      "tslint --fix"
    ]
  },
  "types": "./typings/index.d.ts"
}
